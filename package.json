    "build:pre": "pnpm run build:mp-weixin:pre && pnpm run upload:mp-weixin:pre",
    "build:prod": "pnpm run build:mp-weixin:prod && pnpm run upload:mp-weixin:prod",
    "type-check": "vue-tsc --noEmit",
